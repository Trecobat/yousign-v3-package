{
    "name": "trecobat/yousign-v3-package",
    "description": "Utilisation de l'API V3 de YOUSIGN.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Trecobat\\YousignV3Package\\": ["src/","tests/"],
            "Trecobat\\YousignV3Package\\Model\\": "src/yousignModel/",
            "Trecobat\\YousignV3Package\\Enums\\": "src/enums/",
            "Trecobat\\YousignV3Package\\Interfaces\\": "src/inferfaces/"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/deprecation-contracts": "^v2.5",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": ">=1.8.5"
    },
    "authors": [
        {
            "name": "Pierre-Yves",
            "email": "pierre-yves.desaulle@trecobat.fr"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "9.6.x-dev"
    }
}
